        (function() {

            setTimeout(function() {

                if (!window.readcube) {

                    var pdfMeta;

                    if (document.querySelectorAll) {
                        pdfMeta = document.querySelectorAll('meta[name="PDF_URL"]')[0].getAttribute('content');
                    } else {
                        var metas = document.getElementsByTagName('meta');
                        for (var i = 0, len = metas.length; i < len; i++) {
                            if (metas[i].getAttribute('name') == 'PDF_URL') {
                                pdfMeta = metas[i].getAttribute('content');
                                break;
                            }
                        }
                    }

                    // Handling 'Back' browser button to go to the article page
                    if (document.referrer) {
                        if (window.history && history.replaceState) {
                            try {
                                history.replaceState(null, null, document.referrer);
                            } catch(ex) {}
                        }
                    }

                    location.href = pdfMeta;
                }
            }, 8000);

            if (document.addEventListener) {

